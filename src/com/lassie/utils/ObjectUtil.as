package com.lassie.utils{	import flash.utils.ByteArray;		public final class ObjectUtil	{		public static function assimilate(obj:Object, src:Object):void		{			for (var j:String in src)			{				try				{					if (obj.hasOwnProperty(j)) obj[j] = src[j];				}				catch (e)				{					// create property on source if creation fails.					//if (obj[j] != null) src[j] = obj[j];					trace("Could not assimilate property: " + j);				}			}		}				public static function clone(obj:Object):* 		{			var copy:ByteArray = new ByteArray();			copy.writeObject(obj);			copy.position = 0;			return copy.readObject();		}	}}
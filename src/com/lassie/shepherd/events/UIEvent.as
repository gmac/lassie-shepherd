package com.lassie.shepherd.events{	import flash.events.Event;		public final class UIEvent extends Event	{		public static const ADD:String = "UIAddAction";				public static const COPY:String = "UICopyAction";				public static const DELETE:String = "UIDeleteAction";				public static const REFRESH:String = "UIRefresh";				public static const LOAD:String = "UILoadAction";				public static const SAVE:String = "UISaveAction";				public static const CANCEL:String = "UICancelAction";				public static const LAUNCH:String = "UILaunchAction";				public static const UPLOAD:String = "UIUploadAction";				public static const REFRESH_SCROLL:String = "UIRefreshScroll";				public function UIEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false):void		{			super(type, bubbles, cancelable);		}	}}
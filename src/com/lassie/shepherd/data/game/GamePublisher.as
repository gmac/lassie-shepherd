package com.lassie.shepherd.data.game{	import com.lassie.shepherd.data.XMLPublisher;	import com.lassie.shepherd.core.IServicePackage;		public final class GamePublisher extends XMLPublisher	{		public function GamePublisher(services:IServicePackage):void		{			super(services, GameData.instance.rawData, "Publishing global data...");		}				override protected function compileXML():void		{			saveXML(GameData.instance.id, GameXMLCompiler.parse(GameData.instance));			saveTranscript(GameData.instance.id, GameXMLCompiler.transcript);		}	}}
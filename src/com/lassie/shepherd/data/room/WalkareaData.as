package com.lassie.shepherd.data.room{	public class WalkareaData extends Object	{		private static var spawnIncrement:int = 0;				public var id:String="walkarea";		public var lockId:Boolean = false;		public var spawnNode:int=0;		public var spawnBox:int=0;		public var nodes:Array = new Array();		public var boxes:Array = new Array();		public var lockNodes:Boolean = false;		public var lockBoxes:Boolean = false;		public var showNodes:Boolean = true;		public var showBoxes:Boolean = true;		public var active:Boolean = true;				public function WalkareaData(i:String=""):void		{			if (i == "")			{				spawnIncrement++;				id += spawnIncrement;			}			else			{				id = i;			}		}	}}
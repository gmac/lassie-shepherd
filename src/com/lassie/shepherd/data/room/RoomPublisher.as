package com.lassie.shepherd.data.room{	import com.lassie.shepherd.data.XMLPublisher;	import com.lassie.shepherd.core.IServicePackage;		public final class RoomPublisher extends XMLPublisher	{		public function RoomPublisher(services:IServicePackage):void		{			super(services, RoomData.instance.rawData, "Publishing room data...");		}				override protected function compileXML():void		{			saveXML(RoomData.instance.id, RoomXMLCompiler.parse(RoomData.instance));			saveTranscript(RoomData.instance.id, RoomXMLCompiler.transcript);		}	}}
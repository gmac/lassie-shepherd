package com.lassie.shepherd.data.room{	public class FilterData extends Object	{		private static var _spawn:int = 0;				public var id:String="";		public var lockId:Boolean=false;		public var axis:String=FilterAxis.Y;		public var enabled:Boolean=true;		public var x0:int=-1;		public var y0:int=-1;		public var x1:int=-1;		public var y1:int=-1;		public var scale0:Number=0.5;		public var scale1:Number=1;		public var speed0:Number=0.5;		public var speed1:Number=1;		public var blur0:Number=1;		public var blur1:Number=5;		public var tint0:Number=0.5;		public var tint1:Number=0.5;		public var color0:uint=0x000000;		public var color1:uint=0xFFFFFF;				public function FilterData(filterId:String=""):void		{			super();			id = (filterId == "") ? "filter" + (_spawn++) : filterId;		}	}}
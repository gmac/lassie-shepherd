package com.lassie.shepherd.core{	import flash.events.IEventDispatcher;	import flash.display.MovieClip;	import flash.media.Sound;		public interface ILibraryManager extends IEventDispatcher	{		function getUniqueLoaderName():String;		function purgeAndLoad($loadName:String, $csv:String):void;		function load($loadName:String, $csv:String):void;		function unload($csv:String):void;		function addLoadListener($loadName:String, $event:String, $handler:Function):void;		function removeLoadListener($loadName:String, $event:String, $handler:Function):void;		function hasLibrary($libkey:String, $checkPending:Boolean=false):Boolean;		function hasAllLibraries($csv:String):Boolean;		function getLibrariesList():Array;				// content lists		function getLibraryMovieClips($libkey:String):Array;		function getLibrarySounds($libkey:String):Array;		function getLibraryXML($libkey:String):Array;				// MovieClip accessors		function getMovieClip($libkey:String, $class:String):MovieClip;		function getMovieClipByAddress($address:String):MovieClip;				// Sound accessors		function getSound($libkey:String, $class:String):Sound;		function getSoundByAddress($address:String):Sound;				// XML accessors		function getXML($libkey:String, $id:String):XML;		function getXMLByAddress($address:String):XML;	}}
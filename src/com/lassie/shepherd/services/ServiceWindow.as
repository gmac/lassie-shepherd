package com.lassie.shepherd.services{	import com.lassie.shepherd.ui.Window;	import com.lassie.shepherd.events.ServiceEvent;		public class ServiceWindow extends Window	{	// --------------------------------------------------	//  Constructor	// --------------------------------------------------				public function ServiceWindow():void		{			super();		}			// --------------------------------------------------	//  Methods	// --------------------------------------------------				public override function close():void		{			dispatchEvent(new ServiceEvent(ServiceEvent.CLOSE_SERVICE, true));		}	}}
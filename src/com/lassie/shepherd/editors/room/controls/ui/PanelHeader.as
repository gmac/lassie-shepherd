package com.lassie.shepherd.editors.room.controls.ui{	import flash.display.Sprite;	import flash.text.TextField;		public class PanelHeader extends Sprite	{		public var titleText:TextField;				public function PanelHeader():void		{			super();		}				public function get title():String		{			return titleText.text;		}				public function set title(txt:String):void		{			titleText.text = txt.toUpperCase();		}	}}
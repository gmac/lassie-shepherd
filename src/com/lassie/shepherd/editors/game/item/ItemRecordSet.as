package com.lassie.shepherd.editors.game.item{	import com.lassie.shepherd.editors.game.GameRecordSet;	import com.lassie.shepherd.data.game.ItemRecord;	import com.lassie.shepherd.data.game.GameDataParser;	import com.lassie.shepherd.data.game.GameData;	import com.lassie.shepherd.data.ActionsDataParser;	import com.lassie.utils.ObjectUtil;	import flash.events.Event;		public final class ItemRecordSet extends GameRecordSet	{		public function ItemRecordSet():void		{			super();		}				public override function addRecord():void		{			fileNewRecord(new ItemRecord("item" + numRecords));		}				public override function copyRecord():void		{			ActionsDataParser.voiceIncrement = GameData.instance.voiceInc;			ActionsDataParser.reassignVoiceIncrement = true;						var copy:ItemRecord = GameDataParser.toItemRecord(ObjectUtil.clone(selectedRecord));						ActionsDataParser.reassignVoiceIncrement = false;			GameData.instance.voiceInc = ActionsDataParser.voiceIncrement;						copy.id += "Copy";			fileNewRecord(copy);		}	}}
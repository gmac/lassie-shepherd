package com.lassie.shepherd.editors.game.combo{	import com.lassie.shepherd.editors.game.GameRecordSet;	import com.lassie.shepherd.data.game.GameDataParser;	import com.lassie.shepherd.data.game.ComboRecord;	import com.lassie.shepherd.data.game.GameData;	import com.lassie.shepherd.data.ActionsDataParser;	import com.lassie.utils.ObjectUtil;	import flash.events.Event;		public final class ComboRecordSet extends GameRecordSet	{		public function ComboRecordSet():void		{			super();		}				public override function addRecord():void		{			fileNewRecord(new ComboRecord("Combo" + numRecords));		}				public override function copyRecord():void		{			ActionsDataParser.voiceIncrement = GameData.instance.voiceInc;			ActionsDataParser.reassignVoiceIncrement = true;						var copy:ComboRecord = GameDataParser.toComboRecord(ObjectUtil.clone(selectedRecord));						ActionsDataParser.reassignVoiceIncrement = false;			GameData.instance.voiceInc = ActionsDataParser.voiceIncrement;						copy.id += "Copy";			fileNewRecord(copy);		}	}}
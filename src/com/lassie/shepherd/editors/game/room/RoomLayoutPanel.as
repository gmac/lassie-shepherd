package com.lassie.shepherd.editors.game.room{	import com.lassie.shepherd.editors.game.ui.Panel;	import com.lassie.shepherd.editors.game.ui.PanelDisplays;	import com.lassie.shepherd.events.ShepherdEvent;	import fl.controls.Button;	import flash.events.Event;	import flash.events.MouseEvent;		public final class RoomLayoutPanel extends Panel	{		public var editLayout:Button;			public function RoomLayoutPanel():void		{			super("Edit Room Layout", PanelDisplays.ROOM_LAYOUT);			editLayout.addEventListener(MouseEvent.CLICK, this._onEditRoom);		}			// --------------------------------------------------	//  Event handlers	// --------------------------------------------------				private function _onEditRoom(evt:Event):void		{			dispatchEvent(new ShepherdEvent(ShepherdEvent.LOAD_ROOM, true));		}	}}
package com.lassie.shepherd.ui.window{	import flash.display.Sprite;		public class WindowClose extends Sprite	{		public function WindowClose():void		{			super();			redraw();		}				private function redraw(hover:Boolean=false):void		{			graphics.lineStyle(1, 0x666666, 1);			graphics.beginFill(0x000000, 1);			graphics.drawRect(-20, 0, 8, 8);			graphics.endFill();			graphics.moveTo(-19, 1);			graphics.lineTo(-14, 7);			graphics.moveTo(-19, 7);			graphics.lineTo(-14, 1);		}	}}
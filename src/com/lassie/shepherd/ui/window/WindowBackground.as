package com.lassie.shepherd.ui.window{	import flash.display.Sprite;		public class WindowBackground extends Sprite	{		public function WindowBackground():void		{			super();		}				public function clear():void		{			graphics.clear();		}				public function redraw(w:int, h:int):void		{			graphics.clear();			graphics.lineStyle(1, 0x000000, 1, true);			graphics.beginFill(0x000000, 0.75);			graphics.drawRoundRect(0, 0, w, h, 15);			graphics.endFill();		}	}}
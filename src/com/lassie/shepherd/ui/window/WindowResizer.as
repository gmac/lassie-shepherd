package com.lassie.shepherd.ui.window{	import flash.display.Sprite;		public class WindowResizer extends Sprite	{		public function WindowResizer():void		{			super();						graphics.beginFill(0x000000, 1);			graphics.moveTo(1, 1);			graphics.lineTo(1, -20);			graphics.lineTo(-20, 1);			graphics.endFill();						graphics.lineStyle(1, 0x666666, 1);			graphics.moveTo(-15, -2);			graphics.lineTo(-2, -15);			graphics.moveTo(-10, -2);			graphics.lineTo(-2, -10);			graphics.moveTo(-5, -2);			graphics.lineTo(-2, -5);		}	}}
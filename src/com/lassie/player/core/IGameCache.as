/*** Lassie Engine Game Player* @author Greg MacWilliam.*/package com.lassie.player.core{	import flash.events.IEventDispatcher;	import com.lassie.player.model.Cache;	/**	* IGameCache defines properties and methods of the data caching mechanism.	*/	public interface IGameCache extends IEventDispatcher	{		function load($appId:String):void;		function reset():void;		function get savedGamesList():Array;		function get sessionIndex():int;		function get graphicsQuality():String;		function set graphicsQuality($quality:String):void;		function get fullScreenMode():String;		function set fullScreenMode($mode:String):void;		function get languagePref():String;		function set languagePref($lang:String):void;		function get hasAcceptedLegal():Boolean;		function set hasAcceptedLegal($accept:Boolean):void;		function get hasAutoSave():Boolean;		function get savedRoomPosition():String		function autoSave($position:String=""):void;		function autoResume():void;		function getSessionData():Object;		function getSession():Cache;		function getSessionVars():Cache;		function newData():void;		function loadData($index:uint):void;		function saveData($index:uint, $saveName:String=""):void;		function importData($data:Object, $index:uint=0, $newName:String=""):void;		function clearData($index:uint):void;		function echo():void;	}}
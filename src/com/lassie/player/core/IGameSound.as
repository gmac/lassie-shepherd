/*** Lassie Engine Game Player* @author Greg MacWilliam.*/package com.lassie.player.core{	import flash.events.IEventDispatcher;	import com.gmac.sound.ISoundObject;		/**	* IGameSound defines methods of the LassiePlayer's sound control panel.	*/	public interface IGameSound extends IEventDispatcher	{		function load():void;		function play():void;		function stop():void;		function toggle():void;		function get channel1():String;		function set channel1($libPath:String):void;		function get channel2():String;		function set channel2($libPath:String):void;		function playVoice($libPath:String):ISoundObject;		function playSound($libPath:String):ISoundObject;	}}
/*** Lassie Engine Game Player* @author Greg MacWilliam.*/package com.lassie.player.core{	import flash.events.IEventDispatcher;	/**	* IKeyboardInput defines controls available for the LassiePlayer's keyboard manager.	*/	public interface IKeyboardInput extends IEventDispatcher	{		function setEnabledStatus($key:String, $enabled:Boolean):void;		function addKeyScript($keyValue:String, $script:XML, $branch:Boolean=false):void;		function removeKeyScript($keyValue:String):void;		function setKeyEnabled($keyValue:String, $enable:Boolean):void;		function getKeyEnabled($keyValue:String):Boolean;		function get enabled():Boolean;		function set enabled($enable:Boolean):void;		function get systemEnabled():Boolean;		function set systemEnabled($enable:Boolean):void;	}}
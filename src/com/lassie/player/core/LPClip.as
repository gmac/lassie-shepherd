/*** Lassie Engine Game Player* @author Greg MacWilliam.*/package com.lassie.player.core{	import com.lassie.external.ILassiePlayer;	import com.lassie.external.ILPDisplaySocket;	import com.lassie.player.display.BaseClip;	import com.lassie.player.LassiePlayer;	import com.lassie.player.model.GameModel;	import flash.display.Stage;	import flash.display.DisplayObject;		public class LPClip extends BaseClip implements ILPFramework, ILPDisplaySocket	{		public function LPClip():void {			super();		}				public function get lassiePlayer():ILassiePlayer {			return LassiePlayer.controller;		}				override public function get stage():Stage {			return LassiePlayer.stage;		}				override public function get root():DisplayObject {			return LassiePlayer.root;		}				public function get gameSystem():IGameSystem {			return LassiePlayer.system;		}				public function get gameCache():IGameCache {			return LassiePlayer.cache;		}				public function get gameModel():GameModel {			return LassiePlayer.model;		}				public function get gameController():IGameController {			return LassiePlayer.controller;		}				public function get gameDisplay():GameDisplay {			return LassiePlayer.display;		}				public function get gameLibrary():IGameLibrary {			return LassiePlayer.library;		}		public function get gameSound():IGameSound {			return LassiePlayer.sound;		}				public function get gameDebug():IGameDebugger {			return LassiePlayer.debug;		}				public function get uiKeyboard():IKeyboardInput {			return LassiePlayer.keyboard;		}		public function get uiActionSelector():IActionSelector {			return LassiePlayer.actionSelector;		}				public function get uiContext():IContext {			return LassiePlayer.context;		}				public function get uiCursor():ICursor {			return LassiePlayer.cursor;		}				public function get uiCurtain():ICurtain {			return LassiePlayer.curtain;		}				public function get uiGameMenu():IGameMenu {			return LassiePlayer.gameMenu;		}				public function get uiInventory():IInventory {			return LassiePlayer.inventory;		}				public function get uiLayouts():ILayoutManager {			return LassiePlayer.layouts;		}				public function get uiSubtitle():ISubtitle {			return LassiePlayer.subtitle;		}				public function get uiTreeMenu():ITreeMenu {			return LassiePlayer.treeMenu;		}				public function get uiSkip():ISkipButton {			return LassiePlayer.skip;		}				public function get gameRoom():IRoomDisplay {			return LassiePlayer.room;		}				public function getRoomLayer($id:String):IRoomLayerDisplay {			return LassiePlayer.room.getLayer($id);		}				public function getRoomPuppet($id:String):IPuppetDisplay {			return LassiePlayer.room.getPuppet($id);		}	}}
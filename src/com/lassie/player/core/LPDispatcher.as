/*** Lassie Engine Game Player* @author Greg MacWilliam.*/package com.lassie.player.core{	import com.lassie.player.LassiePlayer;	import com.lassie.player.model.GameModel;	import com.lassie.player.display.GameDisplay;	import flash.events.EventDispatcher;	import flash.display.DisplayObject;	import flash.display.Stage;	public class LPDispatcher extends EventDispatcher implements ILPFramework	{		public function LPDispatcher():void {			super();		}		public function get stage():Stage {			return LassiePlayer.stage;		}		public function get root():DisplayObject {			return LassiePlayer.root;		}		public function get gameSystem():IGameSystem {			return LassiePlayer.system;		}		public function get gameCache():IGameCache {			return LassiePlayer.cache;		}		public function get gameModel():GameModel {			return LassiePlayer.model;		}		public function get gameDisplay():GameDisplay {			return LassiePlayer.display;		}		public function get gameLibrary():IGameLibrary {			return LassiePlayer.library;		}		public function get gameController():IGameController {			return LassiePlayer.controller;		}		public function get gameSound():IGameSound {			return LassiePlayer.sound;		}		public function get gameDebug():IGameDebugger {			return LassiePlayer.debug;		}		public function get uiKeyboard():IKeyboardInput {			return LassiePlayer.keyboard;		}		public function get uiActionSelector():IActionSelector {			return LassiePlayer.actionSelector;		}		public function get uiContext():IContext {			return LassiePlayer.context;		}		public function get uiCursor():ICursor {			return LassiePlayer.cursor;		}		public function get uiCurtain():ICurtain {			return LassiePlayer.curtain;		}		public function get uiGameMenu():IGameMenu {			return LassiePlayer.gameMenu;		}		public function get uiInventory():IInventory {			return LassiePlayer.inventory;		}		public function get uiLayouts():ILayoutManager {			return LassiePlayer.layouts;		}		public function get uiSubtitle():ISubtitle {			return LassiePlayer.subtitle;		}		public function get uiTreeMenu():ITreeMenu {			return LassiePlayer.treeMenu;		}		public function get uiSkip():ISkipButton {			return LassiePlayer.skip;		}		public function get gameRoom():IRoomDisplay {			return LassiePlayer.room;		}		public function getRoomLayer($id:String):IRoomLayerDisplay {			return LassiePlayer.room.getLayer($id);		}		public function getRoomPuppet($id:String):IPuppetDisplay {			return LassiePlayer.room.getPuppet($id);		}	}}